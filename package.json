{
    "name": "Hector",
    "version": "1.0.0-alpha",
    "description": "A discord.js bot that implements little board games (only speaks french as of today)",
    "license": "AGPL-3.0-or-later",
    "keywords": [
        "discord",
        "bot",
        "game"
    ],
    "homepage": "https://github.com/Dettorer/hector#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/Dettorer/hector.git"
    },
    "bugs": "https://github.com/Dettorer/hector/issues",
    "author": {
        "name": "Paul Hervot"
    },
    "type": "module",
    "main": "./hector.js",
    "bin": "./hector.js",
    "dependencies": {
        "discord.js": "^11.5.1",
        "lodash": "^4.17.15"
    },
    "scripts": {
        "build": "babel src -d dist --source-maps && cp config.json dist",
        "start": "npm run build && cd dist && node hector.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/node": "^7.5.5",
        "@babel/preset-env": "^7.5.5"
    },
    "babel": {
        "presets": [[
                "@babel/preset-env", {
                    "targets": {
                        "node": "current"
                    }
                }
        ]],
        "env": {
            "development": {
              "sourceMaps": true,
              "retainLines": true
            }
        }
    }
}
